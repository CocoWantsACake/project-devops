pipeline {
    agent any
	
    environment {
	DOCKER_IMAGE = 'go-image-fromjenkins'
	DOCKER_TAG = 'latest'
    }

    stages {
        stage('Build Docker Image') {
            steps {
                script {
                    sh """
		    docker build -t ${DOCKER_IMAGE}:${DOCKER_TAG} ./webapi
		    """
                }
            }
        }
    }
}
